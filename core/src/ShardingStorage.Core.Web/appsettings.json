{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "DataAccess": {
    "StorageType": "redis",
    "ConfigurationPath": "redis-sharding-config.yaml"
  },
  "RateLimiter": {
    "ConfigurationPath": "rate-limiting-config.yaml"
  }
}
